{
  "name": "moodbox-payload",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build --webpack",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf nod e_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.63.0",
    "@payloadcms/db-vercel-postgres": "3.63.0",
    "@payloadcms/email-nodemailer": "^3.63.0",
    "@payloadcms/live-preview-react": "3.63.0",
    "@payloadcms/next": "3.63.0",
    "@payloadcms/plugin-form-builder": "3.63.0",
    "@payloadcms/plugin-nested-docs": "3.63.0",
    "@payloadcms/plugin-redirects": "3.63.0",
    "@payloadcms/plugin-search": "3.63.0",
    "@payloadcms/plugin-seo": "3.63.0",
    "@payloadcms/richtext-lexical": "3.63.0",
    "@payloadcms/storage-vercel-blob": "^3.63.0",
    "@payloadcms/translations": "^3.64.0",
    "@payloadcms/ui": "3.63.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popper": "^1.2.8",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-form": "^1.23.8",
    "@types/nodemailer": "^7.0.3",
    "@vercel/blob": "^2.0.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "framer-motion": "^12.23.24",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.378.0",
    "next": "16.0.10",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^7.0.10",
    "payload": "3.63.0",
    "prism-react-renderer": "^2.3.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.45.4",
    "react-toastify": "^11.0.5",
    "sharp": "0.34.2",
    "swiper": "^12.0.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@next/eslint-plugin-next": "16.0.10",
    "@playwright/test": "1.56.1",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "16.0.10",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ],
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  }
}
